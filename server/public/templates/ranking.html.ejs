<table class="table table-hover">
	<thead>
		<tr>
			<td>Name</td>
			<td>Party</td>
			<td>Leis orgânicas</td>
			<td>Leis complementares</td>
			<td>Leis ordinárias</td>
		</tr>
	</thead>
	<tbody>
	<% Object.keys(politicians).forEach(function(key){ %>
		<tr>
			<td><%= politicians[key].name %></td>
			<td><%= politicians[key].party %></td>
			<td><%= politicians[key].organic %></td>
			<td><%= politicians[key].complementary %></td>
			<td><%= politicians[key].simple %></td>
		</tr>
	<% }) %>
	</tbody>
</table>
